{
    "config": {
        "platform-check": false,
        "platform": {
            "php": "7.3.0"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "league/flysystem": "^1.1.4",
        "league/flysystem-sftp": "^1.0.22",
        "league/flysystem-webdav": "^1.0",
        "aws/aws-sdk-php": "^3.62",
        "greenlion/php-sql-parser": "^4.6",
        "rmccue/requests": "^1.8",
        "phpmailer/phpmailer": "^6.5.0",
        "mobiledetect/mobiledetectlib": "*",
        "mtdowling/jmespath.php": "^2.4",
        "sabre/uri": "^2.2.1",
        "sendgrid/sendgrid": "^7.0",
        "thenetworg/oauth2-azure": "^2.0",
        "mailgun/mailgun-php": "^2.8",
        "kriswallsmith/buzz": "^1.2",
        "nyholm/psr7": "^1.6.1",
        "php-http/curl-client": "^1.7",
        "php-http/message": "^1.7",
        "google/cloud-storage": "^1.24",
        "google/apiclient": "^2.0",
        "jumbojett/openid-connect-php": "*",
        "guzzlehttp/psr7": "^2.1.1",
        "laravel/serializable-closure": "^1.3",
        "opis/closure": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Duo\\DuoUniversal\\": "./Duo/DuoUniversal",
            "eftec\\bladeone\\": "./BladeOne"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": [
            "Oauth2"
        ]
    }
}
