{
    "presets": [
        ["@babel/preset-env", {
            "modules": false,
            "targets": {
              "browsers": [
                "last 2 versions",
                "IE >= 11"
              ]
            }
          }]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import",
    ]
}